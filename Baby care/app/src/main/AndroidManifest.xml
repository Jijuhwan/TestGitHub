<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="Activity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.helloworld.myapplication.Activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.helloworld.myapplication.Activity.loginActivity" />
        <activity android:name="com.example.helloworld.myapplication.Activity.registActivity" />
        <activity android:name="com.example.helloworld.myapplication.Activity.cancelActivity" />
        <activity android:name="com.example.helloworld.myapplication.zxc.Main2Activity"></activity>
    </application>

</manifest>